{
  "@context": "http://clowder.ncsa.illinois.edu/contexts/extractors.jsonld",
  "name": "ncsa.archival.disk",
  "version": "1.0.1",
  "description": "Archive files from Clowder to another location on disk",
  "author": "Mike Lambert <lambert8@illinois.edu>",
  "contributors": [],
  "contexts": [],
  "repository": [
    {
      "repType": "git",
      "repUrl": "https://git.ncsa.illinois.edu/syngenta/extractors-archival-disk.git"
    },
    {
      "repType": "docker",
      "repUrl": "clowder/extractors-archival-disk"
    }
  ],
  "process": {
    "file": [
      "disk/manualsubmission"
    ]
  },
  "external_services": [],
  "dependencies": [],
  "bibtex": [],
  "categories": [
    "EXTRACT"
  ],
  "parameters": {
    "schema": {
      "properties": {
        "operation": {
          "type": "string",
          "title": "Operation",
          "enum": [
            "archive",
            "unarchive"
          ],
          "default": "archive"
        }
      }
    },
    "form": [
      {
        "key": "operation",
        "type": "radiobuttons",
        "activeClass": "btn-success",
        "default": "archive",
        "notitle": true,
        "titleMap": {
          "archive": "Archive",
          "unarchive": "Unarchive"
        }
      }
    ],
    "value": {
      "operation": "archive"
    }
  },
  "changelog": {
    "1.0.0": [
      "initial release"
    ]
  }
}
